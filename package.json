    "element-resize-detector": "^1.1.16",
    "husky": "^4.0.7",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "refractor": "^2.10.0",
    "reskript": "^0.25.1",
    "webpack": "^4.41.5"
